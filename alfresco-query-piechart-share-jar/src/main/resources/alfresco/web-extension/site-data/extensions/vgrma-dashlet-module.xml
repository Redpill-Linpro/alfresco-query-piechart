<extension>
    <modules>
        <module>
          <id>VGRMA Add overview Dashlets and widgets</id>
          <auto-deploy>true</auto-deploy>
          <version>${project.version}</version>
          <configurations>
              <config evaluator="string-compare" condition="WebFramework" replace="false">
                  <web-framework>
                      <dojo-pages>
                          <packages>
                              <package name="querypiechart" location="js/querypiechart"/>
                          </packages>
                      </dojo-pages>
                  </web-framework>
              </config>
          </configurations>
        </module>
    </modules>
</extension>